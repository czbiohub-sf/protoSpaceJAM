wget https://s3-us-west-2.amazonaws.com/human-pangenomics/T2T/CHM13/assemblies/analysis_set/chm13v2.0.fa.gz --directory-prefix ../genome_files
python 1.0.get_all_human_gRNA.py --fastagz chm13v2.0.fa.gz
